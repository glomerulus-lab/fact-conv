    "import torch\n",
    "import matplotlib.pyplot as plt\n",
    "import os\n",
    "epoch = []\n",
    "for i in range(91):\n",
    "    epoch.append(i)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 27,
